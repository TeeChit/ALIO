<?xml version="1.0" encoding="GB2312"?>
<MonitorConfig  Version="MTK_MonitorConfig_V1" Time="20191105">
    <BigDataConfig Enable="1">
        <Monitor Type="PP_foreground" BigData="1"/> <!-- "PP_foreground" means CAMERA_FRONT -->
        <Monitor Type="PP_background" BigData="1"/> <!-- "PP_background" means CAMERA_BACK -->
        <Monitor Type="v2_version" BigData="1"/> <!-- "v2_version" means new version bigdata -->
    </BigDataConfig>

    <BootConfig>
        <Default>
            <Switch>1</Switch>
            <Memory>1</Memory>
            <Threshold>100</Threshold>
        </Default>
        <ScanApp>
            <AppPackageName>com.eg.android.AlipayGphone</AppPackageName>
            <AppPackageName>com.sankuai.meituan</AppPackageName>
            <AppPackageName>com.tencent.mm</AppPackageName>
            <AppPackageName>tv.danmaku.bili</AppPackageName>
        </ScanApp>
        <FaceUnlockApp>
            <AppPackageName>com.hihonor.securityserver</AppPackageName>
        </FaceUnlockApp>
        <CameraModeApp>
            <AppPackageName>com.hihonor.camera</AppPackageName>
        </CameraModeApp>
        <BootItem>
            <Item Name="ScanApp"  DefaultErrorThreshold="100">
               <TimeOutThreshold AppPackageName="com.eg.android.AlipayGphone" Error="100" />
               <TimeOutThreshold AppPackageName="com.sankuai.meituan" Error="100" />
               <TimeOutThreshold AppPackageName="com.tencent.mm" Error="100" />
               <TimeOutThreshold AppPackageName="tv.danmaku.bili" Error="100" />
            </Item>
            <Item Name="FaceUnlockApp" DefaultErrorThreshold="100">
               <TimeOutThreshold AppPackageName="com.hihonor.securityserver" Error="100" />
            </Item>
            <Item Name="CameraModeApp" DefaultErrorThreshold="100">
                <TimeOutThreshold AppPackageName="com.hihonor.camera"  Error="100" />
            </Item>
        </BootItem>
    </BootConfig>
</MonitorConfig>
