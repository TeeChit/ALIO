service drm /system/bin/drmserver
    disabled
    class main
    user drm
    group drm system inet drmrpc readproc media_rw sdcard_rw
    task_profiles ProcessCapacityHigh HighPerformance BlkioFg

on property:drm.service.enabled=true
    start drm

on property:drm.service.enabled=1
    start drm

#Domestic don't start drmserver
on property:msc.config.optb=156
    stop drm
