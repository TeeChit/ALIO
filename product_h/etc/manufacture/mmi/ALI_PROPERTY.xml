    <Property support="MMI2" value="interrupt" key="ro.mmi.slave_fingerprint_test_type" description="屏下指纹测试子项"/>
    <Property support="MMI2" value="0" key="ro.mmi.slave_fingerprint_start_type" description="屏下光学指纹的启动方式,2代表通过按压音量下键启动"/>
    <Property support="MM2S" value="interrupt" key="ro.mmi.slave_fingerprint_test_type" description="屏下指纹测试子项"/>
    <Property support="MM2S" value="0" key="ro.mmi.slave_fingerprint_start_type" description="屏下光学指纹的启动方式,2代表通过按压音量下键启动"/>
    <Property support="MMIBB" value="interrupt" key="ro.mmi.slave_fingerprint_test_type" description="屏下指纹测试子项"/>
    <Property support="MMIBB" value="0" key="ro.mmi.slave_fingerprint_start_type" description="屏下光学指纹的启动方式,2代表通过按压音量下键启动"/>
