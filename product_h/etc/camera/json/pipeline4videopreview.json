{
    "imports" : [
        "/system_ext/lib64/libhwfilter_qcom.so"
    ],

    "filters" :
    [
        {
            "id" : "src",
            "type" : "camerasource",
            "previewQueueLength" : 8
        },

        {
            "id" : "preview",
            "type" : "preview"
        },

        {
            "id": "bridgesink",
            "type": "bridgesink"
        },

        {
            "id" : "peekerMeta",
            "type" : "peek"
        },

        {
            "id" : "metadata",
            "type" : "qcom_metadata_extractor"
        }
    ],

    "connections" :
    [
        {
            "from" :
            {
                "filter" : "src",
                "port" : "out0"
            },

            "to" :
            {
                "filter" : "preview",
                "port" : "in"
            }
        },

        {
            "from" :
            {
                "filter" : "src",
                "port" : "out_meta"
            },

            "to" :
            {
                "filter" : "peekerMeta",
                "port" : "in"
            }
        },

        {
            "from" :
            {
                "filter" : "peekerMeta",
                "port" : "out"
            },

            "to" :
            {
                "filter" : "preview",
                "port" : "in_meta"
            }
        },

        {
            "from" :
            {
                "filter" : "peekerMeta",
                "port" : "peek0"
            },

            "to" :
            {
                "filter" : "metadata",
                "port" : "in"
            }
        },

        {
            "from" :
            {
                "filter" : "metadata",
                "port" : "out"
            },

            "to" :
            {
                "filter" : "bridgesink",
                "port" : "in_meta"
            }
        },

        {
            "from" :
            {
                "filter" : "src",
                "port" : "out_cap0"
            },

            "to" :
            {
                "filter" : "bridgesink",
                "port" : "in0"
            }
        }
    ]
}

