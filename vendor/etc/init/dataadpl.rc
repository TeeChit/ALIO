# Create the directories used by DPM subsystem

#start dataadpl service
service vendor.dataadpl /system/vendor/bin/adpl
    class main
    user radio
    socket adpl_cmd_uds_file dgram 660 radio radio
    group radio oem_2901 diag usb inet
    disabled

# DTS2021120637710, set prop to start dataadpl service, add by hw0042957
on property:persist.logd.chr.dataadpl.enabled=1
    start vendor.dataadpl

on property:persist.logd.chr.dataadpl.enabled=0
    stop vendor.dataadpl
