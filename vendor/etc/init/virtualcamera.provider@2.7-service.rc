service virtualcamera-hal-service /vendor/bin/hw/virtualcamera.provider@2.4-service
    interface android.hardware.camera.provider@2.7::ICameraProvider virtual/0
    interface android.hardware.camera.provider@2.6::ICameraProvider virtual/0
    interface android.hardware.camera.provider@2.5::ICameraProvider virtual/0
    interface android.hardware.camera.provider@2.4::ICameraProvider virtual/0
    interface vendor.honor.hardware.camera.vircamera@1.0::IVirCameraChannel virtualcamera.register
    disabled
    oneshot
    class hal
    user system
    group camera system
    ioprio rt 4
    capabilities SYS_NICE
    writepid /dev/stune/top-app/tasks
