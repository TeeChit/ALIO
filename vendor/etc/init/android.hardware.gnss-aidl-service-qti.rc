service gnss_service /vendor/bin/hw/android.hardware.gnss-aidl-service-qti
    interface aidl android.hardware.gnss.IGnss/default
    interface aidl vendor.qti.gnss.ILocAidlGnss/default
    class hal
    user gps
    group system gps radio vendor_qti_diag

on post-fs-data
    mkdir /data/vendor/gps 0770 gps system
    mkdir /data/vendor/log 0770 gps system
    mkdir /data/vendor/log/gps 0770 gps system
    mkdir /data/vendor/gnss 0770 gps system
    mkdir /data/vendor/gnss/pgps 0770 gps system
    mkdir /data/vendor/gnss/sdm 0775 gps system