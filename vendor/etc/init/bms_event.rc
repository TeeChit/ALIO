on late-fs
    chmod 0440 /sys/devices/virtual/hw_power/vbus/connect_state
    chmod 0660 /sys/class/power_supply/rt_battery/BMS_EVENT_INIT
    chown system system /sys/devices/virtual/hw_power/vbus/connect_state
    chown system system /sys/class/power_supply/rt_battery/BMS_EVENT_INIT

    start bms_event

service bms_event /vendor/bin/bms_event
    user system
    group system
    disabled

on property:vendor.bms_event=init
    start bms_heating
    start ichg_control
    start bms_behavior

on property:vendor.bms_event=start
    start usb_port
    start bms_heating
    start bms_soc
    start ichg_control
    start bms_cycle

on property:vendor.bms_event=stop
    stop usb_port
    stop bms_heating
    stop bms_soc
    stop ichg_control
    stop chargelogcat-c

on property:vendor.bms_event_vote=start
    start bms_auth
    start bms_behavior

on property:vendor.bms_event_vote=stop
    stop bms_auth

on property:vendor.bms_event=init && property:persist.sys.honor.debug.on=0
    start chargelogcat-c

on property:vendor.bms_event=start && property:persist.sys.honor.debug.on=0
    start chargelogcat-c

